<div class="container contacts-index">
  <div class="alert-success">
    <%= flash[:success] unless flash[:success].blank? %>
  </div>
  <div class="alert-danger">
    <%= flash[:error] unless flash[:error].blank? %>
  </div>
  <h3 style="margin-top: 64px">List of Contacts</h3>
  <br/>
  <table id="users-table", data-source="<%= contacts_path(format: :json) %>" class = "table table-striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
        <th>Phone Number</th>
        <th>Company Name</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>
